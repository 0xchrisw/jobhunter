{
  "Comment": "A description of my state machine",
  "StartAt": "Glue DataBrew StartJobRun",
  "States": {
    "Glue DataBrew StartJobRun": {
      "Type": "Task",
      "Resource": "arn:aws:states:::databrew:startJobRun",
      "Parameters": {
        "Name": "dev-linkedin-bot"
      },
      "Next": "Wait - finish stage crawler"
    },
    "Wait - finish stage crawler": {
      "Type": "Wait",
      "Seconds": 300,
      "Next": "StartCrawler-stage"
    },
    "StartCrawler-stage": {
      "Type": "Task",
      "Parameters": {
        "Name": "dev-linkedin-bot"
      },
      "Resource": "arn:aws:states:::aws-sdk:glue:startCrawler",
      "Next": "Wait - final-crawler"
    },
    "Wait - final-crawler": {
      "Type": "Wait",
      "Seconds": 60,
      "End": true
    }
  }
}